<?xml version="1.0" encoding="utf-8" ?>
<configuration>
   <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <log4net>
    <root>
      <appender-ref ref="MemoryAppender"/>
      <level value="DEBUG" />
      <!--(DEBUG WARN ERROR) Warn level means Error + Transaction. Warn level is used for logging errors and error level is used for logging transactions-->
    </root>
    <appender name="MemoryAppender" type="log4net.Appender.MemoryAppender">
    </appender>
  </log4net>
  <appSettings>
    <add key="ConsoleMode" value="ui" /> <!-- UI or Background-->
    <add key="ExecuteProcessName" value="E:\Work\Demo\TestSharePrice.xaml" /><!-- If mode is Background-->
    <add key="Mode" value="Designer" /> <!-- Designer, Robot ...Added for log purpose-->
    <add key="LogToELK" value="false" />
    <add key="LogToDB" value="true" />
    <add key="LogstashUrl" value="http://127.0.0.1:8080/" />
  </appSettings>
