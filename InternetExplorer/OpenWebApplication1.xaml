<sap:ActivityDesigner x:Class="Bot.Activity.InternetExplorer.OpenWebApplication1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sap="clr-namespace:System.Activities.Presentation;assembly=System.Activities.Presentation"
    xmlns:sapv="clr-namespace:System.Activities.Presentation.View;assembly=System.Activities.Presentation" Loaded="Designer_Loaded">
    <sap:ActivityDesigner.Icon>
        <DrawingBrush>
            <DrawingBrush.Drawing>
                <ImageDrawing>
                    <ImageDrawing.Rect>
                        <Rect Location="0,0" Size="16,16" ></Rect>
                    </ImageDrawing.Rect>
                    <ImageDrawing.ImageSource>
                        <BitmapImage UriSource="/BOTDesigner;component/Images/activityIcon.ico" ></BitmapImage>
                    </ImageDrawing.ImageSource>
                </ImageDrawing>
            </DrawingBrush.Drawing>
        </DrawingBrush>
    </sap:ActivityDesigner.Icon>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <!--<Button x:Name="btnMain" Background="#FF092DAB" Height="17" Margin="154,-27,0,0" VerticalAlignment="Top" Width="22" FontSize="18" Grid.Row="1" HorizontalAlignment="Left"  Click="btnLaunch_Click">

        </Button>-->
    </Grid>
</sap:ActivityDesigner>
