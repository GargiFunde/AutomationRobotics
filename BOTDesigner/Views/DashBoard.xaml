                    <Grid.RowDefinitions>
                        <RowDefinition Height="50" />
                        <RowDefinition Height="50" />
                        <RowDefinition Height="50" />
                        <RowDefinition Height="50" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="65"/>
                        <ColumnDefinition Width="0"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Button x:Name="btnBlankSeq" Grid.Column="2" Grid.Row="1" FontSize="18" HorizontalAlignment="Left" Margin="0,8"  Click="btnBlankSeq_Click" BorderBrush="#FFE8E8EC" Background="White" Width="35">
