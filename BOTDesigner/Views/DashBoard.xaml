                            <SolidColorBrush Color="{DynamicResource Color1}"/>
                        </Button.Foreground>
                        <StackPanel Background="White" Margin="4,1,4,1" >
                            <Image Source="/BOTDesigner;component/Resources/Menu/sequence123.png"/>
                            --><!--<TextBlock Margin="-14,0,-33,0" Text="Sequence Diagram" Foreground="Black" FontSize="10" Height="12"/>--><!--
                        </StackPanel>
                    </Button>
                    <Button  Content="Sequence Diagram" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Style="{StaticResource MyButtonStyle}" Click="btnBlankSeq_Click" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Left"  Margin="40,0,0,0" Background="{x:Null}" BorderBrush="{x:Null}" Width="142" FontFamily="Calibri"/>

                    <Button x:Name="btnBlankWF" Grid.Column="2" Grid.Row="2" FontSize="18"  Opacity="1"  HorizontalAlignment="Left" Margin="0,8"  Click="btnBlankWF_Click" BorderBrush="#FFE8E8EC" Background="White" Width="35">
                        <Button.Foreground>
                            <SolidColorBrush Color="{DynamicResource Color1}"/>
                        </Button.Foreground>
                        <StackPanel Background="White" Margin="2,1,2,1" >
                            --><!--<TextBlock Margin="-14,0,-33,0" Text="Sequence Diagram" Foreground="Black" FontSize="10" Height="12"/>--><!--
                            <Image Source="/BOTDesigner;component/Resources/Menu/workflow123.png" OpacityMask="White" />
                        </StackPanel>
                    </Button>
                    <Button  Content="Workflow Diagram" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Style="{StaticResource MyButtonStyle}" Grid.Column="2" Grid.Row="2" HorizontalAlignment="Left"  Margin="40,0,0,0" Background="{x:Null}" BorderBrush="{x:Null}"  Click="btnBlankWF_Click" Width="144" FontFamily="Calibri"/>

                    <Button x:Name="btnBlankSM" Grid.Column="2" Grid.Row="3"  Opacity="1"  FontSize="18" HorizontalAlignment="Left" Margin="0,8" Click="btnBlankSM_Click" BorderBrush="#FFE8E8EC" Background="White" Width="35">
                        <Button.Foreground>
                            <SolidColorBrush Color="{DynamicResource Color1}"/>
                        </Button.Foreground>
                        <StackPanel Background="White" Margin="2,2,2,2">
                            --><!--<TextBlock Margin="-14,0,-33,0" Text="Sequence Diagram" Foreground="Black" FontSize="10" Height="12"/>--><!--
                            <Image Source="/BOTDesigner;component/Resources/Menu/statemachine123.png" OpacityMask="White" Height="25" />
                        </StackPanel>
                    </Button>
                    <Button  Content="StateMachine Diagram" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Style="{StaticResource MyButtonStyle}" Grid.Column="2" Grid.Row="3" HorizontalAlignment="Left"  Margin="40,0,0,0" Background="{x:Null}" BorderBrush="{x:Null}"  Click="btnBlankSM_Click" Width="153" FontFamily="Calibri"/>

                </Grid>
            </Grid>
        </StackPanel>

        <StackPanel Margin="0,0,0,0" Grid.Column="2" Grid.Row="0" Grid.RowSpan="2" Background="#64B5F6">
            <Grid Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Stretch">
                <Grid.RowDefinitions>
                    <RowDefinition Height="50" />
                    <RowDefinition Height="25" />
                    <RowDefinition Height="25" />
                    <RowDefinition Height="25" />
                    <RowDefinition Height="25" />
                    <RowDefinition Height="25" />
                    <RowDefinition Height="25" />
                    <RowDefinition Height="25" />
                    <RowDefinition Height="25" />
                    <RowDefinition Height="25*" />
                </Grid.RowDefinitions>

                <Label x:Name="btnDeveloperHelp" BorderThickness="0" Content="Developer Help" FontSize="14" Grid.Row="0" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" BorderBrush="#FF865FC5" VerticalAlignment="Center" Width="228" Foreground="#f7f7f7" FontFamily="Calibri" FontWeight="Heavy" />
                <TextBlock HorizontalAlignment="Center" FontSize="12" Grid.Row="1" Margin="10,0,0,0"  VerticalAlignment="Center" FontFamily="Calibri"><Hyperlink Click="btnBestPractices_Click" RequestNavigate="Hyperlink_RequestNavigate" Foreground="#FFEFEFF2"><Run Text="Best Practices"/></Hyperlink></TextBlock>
                <TextBlock HorizontalAlignment="Center" FontSize="12" Grid.Row="2" Margin="10,0,0,0" VerticalAlignment="Center"  FontFamily="Calibri" ><Hyperlink Click="btnExcelAutomation_Click" RequestNavigate="Hyperlink_RequestNavigate" Foreground="#FFEFEFF2"><Run Text="Excel automation example"/></Hyperlink></TextBlock>
                <TextBlock HorizontalAlignment="Center" FontSize="12" Grid.Row="3" Margin="10,0,0,0" VerticalAlignment="Center"  FontFamily="Calibri" ><Hyperlink Click="btnWindowsAutomation_Click" RequestNavigate="Hyperlink_RequestNavigate" Foreground="#FFEFEFF2"><Run Text="Windows automation example"/></Hyperlink></TextBlock>
                <TextBlock HorizontalAlignment="Center" FontSize="12" Grid.Row="4" Margin="10,0,0,0" VerticalAlignment="Center"  FontFamily="Calibri" ><Hyperlink Click="btnCustomActivity_Click" RequestNavigate="Hyperlink_RequestNavigate" Foreground="#FFEFEFF2"><Run Text="How to add your own custom activity"/></Hyperlink></TextBlock>
                <TextBlock HorizontalAlignment="Center" FontSize="12" Grid.Row="5" Margin="10,0,0,0" VerticalAlignment="Center"  FontFamily="Calibri" ><Hyperlink Click="btnCustomProperty_Click" RequestNavigate="Hyperlink_RequestNavigate" Foreground="#FFEFEFF2"><Run Text="How to add custom scraping property"/></Hyperlink></TextBlock>
            </Grid>
        </StackPanel>
        --><!--<Activities:ActivityDesigner1 Grid.Column="2" HorizontalAlignment="Left" Margin="411,6,-183,0" Grid.Row="1" VerticalAlignment="Top"/>-->
        <!--<StackPanel Margin="0,0,0,0" Grid.Column="2" Grid.Row="1">
            <StackPanel.Background>
                <SolidColorBrush Color="#FF865FC5"/>
            </StackPanel.Background>
        </StackPanel>--><!--
    </Grid>
</UserControl>-->


